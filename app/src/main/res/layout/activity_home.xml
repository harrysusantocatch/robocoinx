<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.HomeActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textViewUserId"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="userID" />

        <TextView
            android:id="@+id/textViewBalance"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="balance" />

        <TextView
            android:id="@+id/textViewRP"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="reward poin" />

        <TextView
            android:id="@+id/textViewnextRoll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="next roll" />

        <TextView
            android:id="@+id/textViewRPBonus"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="RP bonus" />

        <TextView
            android:id="@+id/textViewBTCBonus"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="BTC bonus" />

        <WebView
            android:id="@+id/webView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="invisible"
            tools:visibility="invisible" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>